<html>
<head>
    <style>
            .custom-btn {
                width: 200px;
                height: 40px;
                color: #fff;
                border-radius: 5px;
                padding: 10px 25px;
                font-family: 'Lato', sans-serif;
                font-weight: 500;
                background: transparent;
                cursor: pointer;
                transition: all 0.3s ease;
                position: relative;
                display: inline-block;
                box-shadow: inset 2px 2px 2px 0px rgba(255,255,255,.5),
                            7px 7px 20px 0px rgba(0,0,0,.1),
                            4px 4px 5px 0px rgba(0,0,0,.1);
                            outline: none;
            }
   
            .btn-1 {
                background: rgb(96,9,240);
                background: linear-gradient(0deg, rgba(96,9,240,1) 0%, rgba(129,5,240,1) 100%);
                border: none;
        	}
   
            .btn-1:before {
                height: 0%;
                width: 2px;
            }
   
            .btn-1:hover {
                box-shadow: 4px 4px 6px 0 rgba(255,255,255,.5),
                    -4px -4px 6px 0 rgba(116, 125, 136, .5), 
                    inset -4px -4px 6px 0 rgba(255,255,255,.2),
                    inset 4px 4px 6px 0 rgba(0, 0, 0, .4);
            }
    </style>
</head>
<body>
    <a href="/dashtimetable"><h1>HOME</h1></a>
        <div>
            <a href="coordinate.html"><button class="custom-btn btn-1">Coordinater Login</button></a>
            <a href="admin.html"><button class="custom-btn btn-1">Admin Login</button></a>
            <a href="faculty.html"><button class="custom-btn btn-1">Faculty Login</button></a>
        </div>
    <div>
        <table border="1" cellspacing="0" cellpadding="5">
            <tr>
                <th>Faculty Name</th>
                <th>8:40-9:30</th>
                <th>9:30-10:20</th>
                <th>10:20-11:10</th>
                <th>11:10-12:00</th>
                <th>12:50-1:40</th>
                <th>1:40-2:30</th>
                <th>2:30-3:20</th>
                <th>3:20-4:10</th>
            </tr>
            <% var k=0; %>
            <% while(k < result.length) { %>
                
            <tr>
                <th><%= result[k].faculty %></th>
                <% for (var i = 1; i <= 8; i++) { %>
                    <% var found = false; %>
                    <% for (var j = 0; j < results.length; j++) { %>
                        <% if (results[j].faculty == result[k].faculty && results[j].hour == i) { %> 
                            <% if(results[j].year != '' && results[j].subject != '') { %>
                                <th><%= results[j].subject %> - <%= results[j].year %><%= results[j].section %></th>
                            <% } else { %>
                                <th>-</th>
                            <% } %>
                            <% found = true; %>
                            <% break; %>
                        <% } %>
                    <% } %>
                    <% if (!found) { %>
                        <th>-</th>
                    <% } %>
                <% } %>  
        </tr>
        <% k++; %>
        <% } %>
            <!-- Add more rows for other faculties -->
        </table>
    </div>
</body>
</html>