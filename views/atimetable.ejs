            <% if (results[i].day === 'Wednesday') { %>
              <th><%= results[i].subject %> (<%= results[i].faculty %>)</th>
            <% } %>
          <% } %>  
      </tr>
      <tr>
        <th>Thusrsday</th>
        <% for (var i = 0; i < results.length; i++) { %>
            <% if (results[i].day === 'Thursday') { %>
              <th><%= results[i].subject %> (<%= results[i].faculty %>)</th>
            <% } %>
          <% } %>  
      </tr>
      <tr>
        <th>Friday</th>
        <% for (var i = 0; i < results.length; i++) { %>
            <% if (results[i].day === 'Friday') { %>
              <th><%= results[i].subject %> (<%= results[i].faculty %>)</th>
            <% } %>
          <% } %>  
      </tr>
      <tr>
        <th>Saturday</th>
        <% for (var i = 0; i < results.length; i++) { %>
            <% if (results[i].day === 'Saturday') { %>
              <th><%= results[i].subject %> (<%= results[i].faculty %>)</th>
            <% } %>
          <% } %>  
      </tr>
    </table>
  </body>
</html>
